{
  "name": "umi-lint",
  "version": "2.0.2",
  "description": "configless lint tool all in one",
  "keywords": [
    "eslint",
    "husky",
    "lint",
    "lint-staged",
    "prettier",
    "stylelint"
  ],
  "repository": {
