    "stylelint",
    "prettier",
    "lint-staged",
    "husky",
    "tslint"
  ],
  "author": {
    "name": "clock157",
    "email": "clock157@163.com"
  },
  "license": "MIT",
  "dependencies": {
    "common-bin": "2.8.1",
    "debug": "^4.1.1",
    "eslint": "^5.14.0",
    "globby": "^9.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "resolve-bin": "^0.4.0",
    "stylelint": "^9.10.1",
    "tslint": "^5.12.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
